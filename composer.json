{
    "name": "combi/core",
    "description": "combi core libs",
    "keywords": ["framework", "core", "libs"],
    "license": "MPL2.0",
    "authors": [
        {
            "name": "Andares Merigold",
            "email": "andares@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-msgpack": "*",
        "ext-redis": "*",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "nette/neon": "^2.4",
        "monolog/monolog": "^1.0"
    },
    "require-dev": {
        "nette/tester": "dev-master"
    },
    "suggest": {
        "nette/di": "Allow use service by DI. (need 'nette/di: ^2.4')",
        "ext-gmp": "Make gen id with helper faster"
    },
    "autoload": {
        "psr-4": {"Combi\\": "src/classes/"},
        "files": [
            "src/bootstrap.php"
        ]
    },
    "minimum-stability": "stable"
}
